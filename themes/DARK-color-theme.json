{
  "$schema": "vscode://schemas/color-theme",
  "name": "DARK",
  "type": "dark",
  "colors": {
    // Base colors - Impossibly dark background that absorbs all light
    "editor.background": "#030305",
    "editor.foreground": "#8D94A0",

    // Sidebar - Even darker than the abyss for maximum darkness
    "sideBar.background": "#020203",
    "sideBar.foreground": "#61676E",
    "sideBar.border": "#0A0B0D",

    // Activity Bar - Darkness beyond darkness
    "activityBar.background": "#010102",
    "activityBar.foreground": "#3E5880",
    "activityBar.border": "#0A0B0D",
    "activityBar.activeBorder": "#8C3840",
    "activityBar.activeBackground": "#050914",

    // Title Bar - Void of light
    "titleBar.activeBackground": "#010102",
    "titleBar.activeForeground": "#61676E",
    "titleBar.inactiveBackground": "#010102",
    "titleBar.inactiveForeground": "#3E4147",

    // Status Bar - Black hole darkness
    "statusBar.background": "#070709",
    "statusBar.foreground": "#61676E",
    "statusBar.noFolderBackground": "#0E070F",
    "statusBar.debuggingBackground": "#0F070F",

    // Editor UI elements - Darker than the darkest night
    "editorGroupHeader.tabsBackground": "#020203",
    "tab.activeBackground": "#030305",
    "tab.activeForeground": "#3E5880",
    "tab.inactiveBackground": "#020203",
    "tab.inactiveForeground": "#4A4E53",
    "tab.activeBorderTop": "#8C3840",

    // Breadcrumbs - Subtle guidance elements
    "breadcrumb.foreground": "#8B949E",
    "breadcrumb.focusForeground": "#D0D7DE",
    "breadcrumb.activeSelectionForeground": "#58A6FF",

    // Line highlighting - Almost imperceptible, as if swallowed by darkness
    "editor.lineHighlightBackground": "#060709",
    "editor.lineHighlightBorder": "#060709",

    // Selection highlighting - Darkest blue that barely allows recognition
    "editor.selectionBackground": "#0E1A30",
    "editor.selectionHighlightBackground": "#0A1526",
    "editor.inactiveSelectionBackground": "#080F19",

    // Word highlighting - Shadows within shadows
    "editor.wordHighlightBackground": "#0E1015",
    "editor.wordHighlightStrongBackground": "#12161A",

    // Find/Replace - Darkest possible amber that's still functional
    "editor.findMatchBackground": "#261407",
    "editor.findMatchHighlightBackground": "#1A0E05",
    "editor.findRangeHighlightBackground": "#0E0F12",

    // Peek view - Darkness with just enough contrast to be usable
    "peekView.border": "#8C3840",
    "peekViewEditor.background": "#050709",
    "peekViewEditor.matchHighlightBackground": "#1E1505",
    "peekViewResult.background": "#020203",
    "peekViewTitle.background": "#050709",

    // Git decorations - Intuitive color coding
    "gitDecoration.addedResourceForeground": "#56D364",
    "gitDecoration.modifiedResourceForeground": "#E3B341",
    "gitDecoration.deletedResourceForeground": "#F97583",
    "gitDecoration.untrackedResourceForeground": "#56D364",
    "gitDecoration.ignoredResourceForeground": "#484F58",
    "gitDecoration.conflictingResourceForeground": "#DB61A2",

    // Terminal colors - Darkness with minimal color for functionality
    "terminal.background": "#030305",
    "terminal.foreground": "#8D94A0",
    "terminal.ansiBlack": "#030305",
    "terminal.ansiBlue": "#3E5880",
    "terminal.ansiBrightBlack": "#3E4147",
    "terminal.ansiBrightBlue": "#4A7099",
    "terminal.ansiBrightCyan": "#387F85",
    "terminal.ansiBrightGreen": "#4A8652",
    "terminal.ansiBrightMagenta": "#7A6499",
    "terminal.ansiBrightRed": "#994940",
    "terminal.ansiBrightWhite": "#A0A6AD",
    "terminal.ansiBrightYellow": "#998538",
    "terminal.ansiCyan": "#267078",
    "terminal.ansiGreen": "#387845",
    "terminal.ansiMagenta": "#5E4D85",
    "terminal.ansiRed": "#8C3840",
    "terminal.ansiWhite": "#8D94A0",
    "terminal.ansiYellow": "#856B30",

    // Panels - Consistent with sidebar for cohesion
    "panel.background": "#090D13",
    "panel.border": "#171B22",
    "panelTitle.activeBorder": "#F97583",
    "panelTitle.activeForeground": "#D0D7DE",
    "panelTitle.inactiveForeground": "#8B949E",

    // Notifications - Designed for attention without being jarring
    "notificationCenter.border": "#171B22",
    "notificationCenterHeader.background": "#0D1117",
    "notificationCenterHeader.foreground": "#D0D7DE",
    "notificationLink.foreground": "#58A6FF",
    "notifications.background": "#0D1117",
    "notifications.border": "#171B22",
    "notifications.foreground": "#D0D7DE",

    // Input controls - Subtle borders for form elements
    "input.background": "#0D1117",
    "input.border": "#30363D",
    "input.foreground": "#D0D7DE",
    "input.placeholderForeground": "#6E7681",
    "inputOption.activeBackground": "#1C2938",
    "inputOption.activeBorder": "#58A6FF",

    // Scrollbar - Designed to be visible but not distracting
    "scrollbar.shadow": "#010409",
    "scrollbarSlider.activeBackground": "#484F5899",
    "scrollbarSlider.background": "#484F5866",
    "scrollbarSlider.hoverBackground": "#484F5899",

    // Badge - Accent color for notification counts
    "badge.background": "#F97583",
    "badge.foreground": "#0D1117",

    // Progress bar - Positive reinforcement through color
    "progressBar.background": "#58A6FF",

    // Lists and trees - Consistent with other UI elements
    "list.activeSelectionBackground": "#1C2938",
    "list.activeSelectionForeground": "#D0D7DE",
    "list.dropBackground": "#1C2938",
    "list.focusBackground": "#21262D",
    "list.focusForeground": "#D0D7DE",
    "list.highlightForeground": "#58A6FF",
    "list.hoverBackground": "#21262D",
    "list.hoverForeground": "#D0D7DE",
    "list.inactiveSelectionBackground": "#21262D",
    "list.inactiveSelectionForeground": "#D0D7DE",
    "listFilterWidget.background": "#21262D",
    "listFilterWidget.noMatchesOutline": "#F97583",
    "listFilterWidget.outline": "#21262D",

    // Dropdown - Consistent with input controls
    "dropdown.background": "#0D1117",
    "dropdown.border": "#30363D",
    "dropdown.foreground": "#D0D7DE",

    // Quick input - Streamlined for fast navigation
    "quickInput.background": "#0D1117",
    "quickInput.foreground": "#D0D7DE",
    "quickInputTitle.background": "#111620"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#3E4147",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#8D94A0"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#4A7099"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#994940"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#994940"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "keyword.control",
        "constant.other.color",
        "punctuation",
        "meta.tag",
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        "foreground": "#4A7099"
      }
    },
    {
      "name": "Tag",
      "scope": ["entity.name.tag", "meta.tag.sgml", "markup.deleted.git_gutter"],
      "settings": {
        "foreground": "#FF7B72"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#58A6FF"
      }
    },
    {
      "name": "Block Level Variables",
      "scope": ["meta.block variable.other"],
      "settings": {
        "foreground": "#D0D7DE"
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": ["support.other.variable", "string.other.link"],
      "settings": {
        "foreground": "#FF7B72"
      }
    },
    {
      "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape",
        "variable.parameter",
        "keyword.other.unit",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#E3B341"
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "entity.other.inherited-class",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "foreground": "#7EE787"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name",
        "support.type",
        "support.class",
        "support.other.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter",
        "support.type.sys-types"
      ],
      "settings": {
        "foreground": "#D2A8FF"
      }
    },
    {
      "name": "Entity Types",
      "scope": ["support.type"],
      "settings": {
        "foreground": "#79C0FF"
      }
    },
    {
      "name": "CSS Class and Support",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
      ],
      "settings": {
        "foreground": "#79C0FF"
      }
    },
    {
      "name": "Sub-methods",
      "scope": ["entity.name.module.js", "variable.import.parameter.js", "variable.other.class.js"],
      "settings": {
        "foreground": "#FF7B72"
      }
    },
    {
      "name": "Language methods",
      "scope": ["variable.language"],
      "settings": {
        "foreground": "#FF7B72",
        "fontStyle": "italic"
      }
    },
    {
      "name": "entity.name.method.js",
      "scope": ["entity.name.method.js"],
      "settings": {
        "foreground": "#58A6FF"
      }
    },
    {
      "name": "meta.method.js",
      "scope": ["meta.class-method.js entity.name.function.js", "variable.function.constructor"],
      "settings": {
        "foreground": "#58A6FF"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#79C0FF"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "text.html.basic entity.other.attribute-name.html",
        "text.html.basic entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#79C0FF",
        "fontStyle": "italic"
      }
    },
    {
      "name": "CSS Classes",
      "scope": ["entity.other.attribute-name.class"],
      "settings": {
        "foreground": "#79C0FF"
      }
    },
    {
      "name": "CSS ID's",
      "scope": ["source.sass keyword.control"],
      "settings": {
        "foreground": "#58A6FF"
      }
    },
    {
      "name": "Inserted",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#7EE787"
      }
    },
    {
      "name": "Deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#FF7B72"
      }
    },
    {
      "name": "Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#D2A8FF"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#56D4DD"
      }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#56D4DD"
      }
    },
    {
      "name": "URL",
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "tag.decorator.js entity.name.tag.js",
        "tag.decorator.js punctuation.definition.tag.js"
      ],
      "settings": {
        "foreground": "#58A6FF",
        "fontStyle": "italic"
      }
    },
    {
      "name": "JSON Key - Level 0",
      "scope": ["source.json meta.structure.dictionary.json support.type.property-name.json"],
      "settings": {
        "foreground": "#79C0FF"
      }
    },
    {
      "name": "JSON Key - Level 1",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#D2A8FF"
      }
    },
    {
      "name": "JSON Key - Level 2",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#58A6FF"
      }
    },
    {
      "name": "JSON Key - Level 3",
      "scope": [
        "source.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json meta.structure.dictionary.value.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#FF7B72"
      }
    },
    {
      "name": "Markdown - Plain",
      "scope": ["text.html.markdown", "punctuation.definition.list_item.markdown"],
      "settings": {
        "foreground": "#D0D7DE"
      }
    },
    {
      "name": "Markdown - Heading",
      "scope": [
        "markdown.heading",
        "markup.heading | markup.heading entity.name",
        "markup.heading.markdown punctuation.definition.heading.markdown"
      ],
      "settings": {
        "foreground": "#58A6FF"
      }
    },
    {
      "name": "Markup - Italic",
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FF7B72"
      }
    },
    {
      "name": "Markup - Bold",
      "scope": ["markup.bold", "markup.bold string"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#FF7B72"
      }
    },
    {
      "name": "Markup - Bold-Italic",
      "scope": [
        "markup.bold markup.italic",
        "markup.italic markup.bold",
        "markup.quote markup.bold",
        "markup.bold markup.italic string",
        "markup.italic markup.bold string",
        "markup.quote markup.bold string"
      ],
      "settings": {
        "fontStyle": "bold italic",
        "foreground": "#FF7B72"
      }
    },
    {
      "name": "Markup - Underline",
      "scope": ["markup.underline"],
      "settings": {
        "fontStyle": "underline",
        "foreground": "#E3B341"
      }
    },
    {
      "name": "Markdown - Blockquote",
      "scope": ["markup.quote punctuation.definition.blockquote.markdown"],
      "settings": {
        "foreground": "#6E7681"
      }
    },
    {
      "name": "Markup - Quote",
      "scope": ["markup.quote"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#8B949E"
      }
    },
    {
      "name": "Markdown - Link",
      "scope": ["string.other.link.title.markdown"],
      "settings": {
        "foreground": "#58A6FF"
      }
    },
    {
      "name": "Markdown - Link Description",
      "scope": ["string.other.link.description.title.markdown"],
      "settings": {
        "foreground": "#D2A8FF"
      }
    },
    {
      "name": "Markdown - Link Anchor",
      "scope": ["constant.other.reference.link.markdown"],
      "settings": {
        "foreground": "#7EE787"
      }
    },
    {
      "name": "Markup - Raw Block",
      "scope": ["markup.raw.block"],
      "settings": {
        "foreground": "#D2A8FF"
      }
    },
    {
      "name": "Markdown - Fenced Bode Block Variable",
      "scope": ["markup.fenced_code.block.markdown", "markup.inline.raw.string.markdown"],
      "settings": {
        "foreground": "#79C0FF"
      }
    },
    {
      "name": "Markdown - Fenced Language",
      "scope": ["variable.language.fenced.markdown"],
      "settings": {
        "foreground": "#6E7681"
      }
    },
    {
      "name": "Markdown - Separator",
      "scope": ["meta.separator"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#6E7681"
      }
    },
    {
      "name": "Markup - Table",
      "scope": ["markup.table"],
      "settings": {
        "foreground": "#79C0FF"
      }
    }
  ]
}
